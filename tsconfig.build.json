{
    "extends": "./tsconfig.json",
    "compilerOptions": {
      "noEmit": false,
      "outDir": "./dist",
      "declaration": true,
      "declarationDir": "./dist/types",
      "emitDeclarationOnly": false,
      "jsx": "react-native",
      "module": "ESNext",
      "target": "ES2017",
      "isolatedModules": true,
      "baseUrl": "./",
      "paths": {
        "mediasoup-client": ["./node_modules/mediasoup-client"],
        "mediasoup-client/*": ["./node_modules/mediasoup-client/*"]
      }
    },
    "include": ["src","./main.tsx"],
    "exclude": ["node_modules", "build", "dist"]
  }
  